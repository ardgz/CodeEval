import sys

lines = file(sys.argv[1], 'r')

for line in lines:
    line = line.strip() # Remove newline char
    numbers = line.split(';')
    
    result = ""

    for num in numbers:
        if num == 'zero':
            result += 0
        elif num == 'one':
            result += 1
        elif num == 'two':
            result += 2
        elif num == 'three':
            result += 3
        elif num == 'four':
            result += 4
        elif num == 'five':
            result += 5
        elif num == 'six':
            result += 6
        elif num == 'seven':
            result += 7
        elif num == 'eight':
            result += 8
        elif num == 'nine':
            result += 9
    print result
            
